{
  "name": "solfail",
  "version": "1.0.0",
  "description": "Minimal Solana transaction failure decoder - Decode transaction failures with clear explanations and actionable fixes",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    },
    "./server": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": "./dist/cli.js"
  },
  "files": [
    "dist",
    "fixtures",
    "README.md",
    "CONTRIBUTING.md",
    "USAGE.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "solfail": "./dist/cli.js"
  },
  "keywords": [
    "solana",
    "transaction",
    "decoder",
    "error",
    "failure",
    "debugging",
    "rpc",
    "simulation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/solfail.git"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.8",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "ts-node": "^10.9.2"
  }
}

